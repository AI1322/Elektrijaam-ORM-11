@model IEnumerable<Elektrijaam_ORM_11.Models.Asukoht>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Tanav)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Maja)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Linn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Postiindeks)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Tanav)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Maja)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Linn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Postiindeks)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
